    "immutable": "^3.8.2",
    "keycode": "^2.2.0",
    "localforage": "^1.7.1",
    "localforage-getitems": "^1.4.1",
    "lodash": "^4.17.4",
    "oauth-open": "^1.0.3",
    "query-string": "5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-ga": "^2.4.0",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
